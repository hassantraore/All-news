
<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    {#  <title>{% block title %}AllNews!{% endblock %}</title>#}
    {% block stylesheets %}
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/css/bootstrap.min.css" integrity="sha384-B0vP5xmATw1+K9KRQjQERJvTumQW0nPEzvF6L/Z6nronJ3oUOFUFpCjEUQouq2+l" crossorigin="anonymous">
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">
    {% endblock %}
</head>
<body>
<style>
.cat:hover{
    background-color:white;
    
}
</style>
 <nav class="navbar navbar-dark  navbar-expand-xl bg-dark" style="height:35px">
    <ul class="navbar-nav ">

        <li class="nav-item">
            <a  class="nav-link active text-muted" href="javascript:void(0)">
                <i class="bi bi-twitter"></i>
            </a>
        </li>
         <li class="nav-item">
            <a  class="nav-link active text-muted" href="javascript:void(0)">
                <i class="bi bi-facebook"></i>
            </a>
        </li>
        <li class="nav-item">
            <a class="nav-link active text-muted" href="javascript:void(0)">
                <i class="bi bi-instagram"></i>
            </a>
        </li>
        
     </ul>
    
    <ul class="navbar-nav ml-auto">
     
      {% if is_granted("ROLE_USER","ROLE_ADMIN") %}


                    <li class="nav-item cat">
                        <a class="nav-link " aria-current="page" href="{{ path("app_logout") }}">Déconnexion</a>
                    </li>



                {% else  %}
                    <li class="nav-item">
                        <a class="nav-link " aria-current="page" href="{{ path("security_login") }}">S'identifier</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link " aria-current="page" href="{{ path("registration") }}">S'inscrire</a>
                    </li>

                {%  endif  %}

    </ul>
  
  </nav>
<nav class="navbar navbar-expand-lg navbar-light bg-warning">
    <div class="container-fluid">
        <a class="navbar-brand" href="#">AllNEWS</a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                <li class="nav-item">
                    <a class="nav-link active cat"  href="{{ path("home") }}">FIL D'ACTUALITE</a>
                </li>



                <li class="nav-item">
                    <a class="nav-link active cat" aria-current="page" href="{{ path("economy") }}">ECONOMIE</a>
                </li>


                <li class="nav-item">
                    <a class="nav-link active cat" aria-current="page" href="{{ path("politics") }}">POLITIQUE</a>
                </li>


                <li class="nav-item cat">
                    <a class="nav-link active cat" aria-current="page" href="{{ path("revue_de_presse_home") }}">FOCUS</a>
                </li>

            </ul>

        </div>
    </div>
</nav>
<div>
    {% block body %} {% endblock %}

</div>


{% block  javascripts %}
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/js/bootstrap.bundle.min.js" integrity="sha384-JEW9xMcG8R+pH31jmWH6WWP0WintQrMb4s7ZOdauHnUtxwoG2vI5DkLtS3qm9Ekf" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.1/dist/umd/popper.min.js" integrity="sha384-SR1sx49pcuLnqZUnnPwx6FCym0wLsk5JZuNx2bPPENzswTNFaQU1RDvt3wT4gWFG" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/js/bootstrap.min.js" integrity="sha384-j0CNLUeiqtyaRmlzUHCPZ+Gy5fQu0dQ6eZ/xAww941Ai1SxSY+0EQqNXNE6DZiVc" crossorigin="anonymous"></script>
{% endblock %}

